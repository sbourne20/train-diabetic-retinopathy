{
  "data": {
    "dataset_path": "./dataset_aptos",
    "num_classes": 5,
    "img_size": 224,
    "batch_size": 16,
    "enable_clahe": false,
    "clahe_clip_limit": 3.0,
    "clahe_tile_grid_size": [
      8,
      8
    ]
  },
  "model": {
    "base_models": [
      "mobilenet_v2"
    ],
    "freeze_weights": true,
    "dropout": 0.5,
    "num_classes": 5
  },
  "training": {
    "epochs": 50,
    "learning_rate": 0.0005,
    "weight_decay": 0.001,
    "patience": 5,
    "enable_focal_loss": true,
    "enable_class_weights": true,
    "focal_loss_alpha": 2.0,
    "focal_loss_gamma": 3.0,
    "scheduler": "cosine",
    "warmup_epochs": 5,
    "rotation_range": 25.0,
    "brightness_range": 0.2,
    "contrast_range": 0.2,
    "resume": false
  },
  "paths": {
    "output_dir": "./aptos_results"
  },
  "system": {
    "device": "cuda",
    "seed": 42,
    "output_dir": "./aptos_results",
    "experiment_name": "aptos_2019_mobilenet_antioverfitting",
    "target_accuracy": 0.9,
    "use_wandb": true
  }
}