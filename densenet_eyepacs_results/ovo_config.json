{
  "data": {
    "dataset_path": "./dataset_eyepacs",
    "num_classes": 5,
    "img_size": 299,
    "batch_size": 12,
    "enable_clahe": false,
    "clahe_clip_limit": 3.0,
    "clahe_tile_grid_size": [
      8,
      8
    ]
  },
  "model": {
    "base_models": [
      "densenet121"
    ],
    "freeze_weights": false,
    "dropout": 0.2,
    "num_classes": 5
  },
  "training": {
    "epochs": 80,
    "learning_rate": 0.0002,
    "weight_decay": 0.0005,
    "patience": 15,
    "enable_focal_loss": true,
    "enable_class_weights": true,
    "focal_loss_alpha": 3.0,
    "focal_loss_gamma": 4.0,
    "scheduler": "cosine",
    "warmup_epochs": 5,
    "rotation_range": 25.0,
    "brightness_range": 0.2,
    "contrast_range": 0.2,
    "resume": false
  },
  "paths": {
    "output_dir": "./densenet_eyepacs_results"
  },
  "system": {
    "device": "cuda",
    "seed": 42,
    "output_dir": "./densenet_eyepacs_results",
    "experiment_name": "eyepacs_densenet121_extreme_optimized",
    "target_accuracy": 0.9,
    "use_wandb": true
  }
}