{
  "data": {
    "dataset_path": "./dataset_eyepacs",
    "num_classes": 5,
    "img_size": 448,
    "batch_size": 8,
    "enable_clahe": false,
    "clahe_clip_limit": 3.0,
    "clahe_tile_grid_size": [
      8,
      8
    ]
  },
  "model": {
    "base_models": [
      "medsiglip_448"
    ],
    "freeze_weights": false,
    "dropout": 0.2,
    "num_classes": 5
  },
  "training": {
    "epochs": 60,
    "learning_rate": 0.0001,
    "weight_decay": 0.0003,
    "patience": 15,
    "enable_focal_loss": true,
    "enable_class_weights": true,
    "focal_loss_alpha": 2.5,
    "focal_loss_gamma": 3.5,
    "scheduler": "plateau",
    "warmup_epochs": 5,
    "rotation_range": 15.0,
    "brightness_range": 0.1,
    "contrast_range": 0.1,
    "resume": false
  },
  "paths": {
    "output_dir": "./medsiglip_results"
  },
  "system": {
    "device": "cuda",
    "seed": 42,
    "output_dir": "./medsiglip_results",
    "experiment_name": "eyepacs_medsiglip_augmented_optimized",
    "target_accuracy": 0.92,
    "use_wandb": true
  }
}