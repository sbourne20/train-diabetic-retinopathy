{
  "data": {
    "dataset_path": "./dataset_eyepacs",
    "num_classes": 5,
    "img_size": 299,
    "batch_size": 10,
    "enable_clahe": true,
    "clahe_clip_limit": 2.5,
    "clahe_tile_grid_size": [
      8,
      8
    ]
  },
  "model": {
    "base_models": [
      "densenet121"
    ],
    "freeze_weights": false,
    "dropout": 0.3,
    "num_classes": 5
  },
  "training": {
    "epochs": 100,
    "learning_rate": 0.0001,
    "weight_decay": 0.0003,
    "patience": 25,
    "enable_focal_loss": true,
    "enable_class_weights": true,
    "focal_loss_alpha": 2.5,
    "focal_loss_gamma": 3.0,
    "scheduler": "cosine",
    "warmup_epochs": 10,
    "rotation_range": 25.0,
    "brightness_range": 0.2,
    "contrast_range": 0.2,
    "label_smoothing": 0.1,
    "class_weight_mild": 12.0,
    "class_weight_moderate": 6.0,
    "class_weight_severe": 10.0,
    "class_weight_pdr": 12.0,
    "resume": false
  },
  "paths": {
    "output_dir": "./densenet_eyepacs_results"
  },
  "system": {
    "device": "cuda",
    "seed": 42,
    "output_dir": "./densenet_eyepacs_results",
    "experiment_name": "eyepacs_densenet121_balanced_clahe",
    "target_accuracy": 0.88,
    "use_wandb": true
  }
}